<!--Template analysis-->
    <div class="row">
      <div class="col-12 col-md-12 col-lg-12 mb-12 mb-lg-0">
          <div class="card">
              <h5 class="card-header">State analysis</h5>
              <div class="card-body">

      <div class="templates">
      <table>
        <thead>
          <tr>
            <th><div style="text-align: center">{{cols[0]}}</div></th>
            <th><div style="text-left: center">{{cols[1]}}</div></th>

            <th class="rotate">
              <div style="max-width: none;">
                <svg width="82" height="82" style="max-width: none;">
                  <text style="#11f" transform="translate(25,60)rotate(-45)" x="0" y="0">{{cols[2]}}</text>
                  <line x1="0" y1="65" x2="25" y2="40" transform="translate(41.5,0)"></line>
                </svg>
              </div>
            </th>
            <th class="rotate">
              <div>
                <svg width="82" height="82" style="max-width: none;">
                  <text transform="translate(25,60)rotate(-45)" x="0" y="0">{{cols[3]}}</text>
                </svg>
              </div>
            </th>

            {% for c in cols[4:9] %}
            <th{% if c.lower() in ['count'] %} colspan="4"{% endif %}>
                <div {% if c.lower() in ['trend', 'change', 'coverage'] %}style="text-align: center"{% endif %}
                     {% if c.lower() in ['count'] %}style="text-align: center"{% endif %}>
                {{c}}
              </div>
            </th>
            {% endfor %}

            <th style="width: 2px"></th>

            {% for c in cols[9:] %}
            <th><div>{{c}}</div></th>
            {% endfor %}

          </tr>
        </thead>

        <tbody>
          {% for row in rows %}
          <tr>
            <td class="risk">
                 <i class="fa {{row.risk_symbol}} font-medium-1" style="color: {{row.risk_color}}"></i>
            </td>

            <td class="description"> {{row.risk_description}} </td>

            <td class="heat">
              <div style="background-color: {{row.b_color}}">{{row.perc_baseline}}%</div>
            </td>

            <td class="heat">
              <div style="background-color: {{row.c_color}}">{{row.perc_candidate}}%</div>
            </td>

            <td class="template">{{row.template}}</td>

            <td class="code">
                {% if row.template_code %}
                 <a href="{{ row.template_code }}" id="profile-link">
                     <i class="fa fa-link secondary font-medium-1"></i>
                 </a>
                {% else %}
                 <a href="https://github.com/willyblackkeez" id="profile-link">
                     <i class="fa fa-external-link font-medium-1" style="color: rgba(0, 0, 0, 0.5)"></i>
                 </a>
                {% endif %}

            </td>

            <td class="count">
              <div class="count">
                    {{row.count_base}}<span style="color: #999">&nbspB</span>
              </div>
            </td>

            <td class="trend">
                <span class="inlinesparkline">{{row.trend_baseline}}</span>
            </td>

            <td class="count">
              <div class="count">
                    {{row.count_cand}}<span style="color: #999">&nbspC</span>
              </div>
            </td>

            <td class="trend">
                {% set lst_trend_candidate = row.trend_candidate.split(',') %}
                {% if row.trend_baseline %}
                      {% if lst_trend_candidate|length > 1 %}
                        <span class="dualsparkline" barvalues={{row.trend_candidate}} linevalues={{row.trend_baseline}}></span>
                      {% endif %}
                {% else %}
                      {% if lst_trend_candidate|length > 1 %}
                        <span class="barsparkline">{{row.trend_candidate}}</span>
                      {% endif %}
                {% endif %}
            </td>

            <td class="change" style="color: {{row.change_color}}">
                {% if row.change_perc >= 0 %}+{% endif %}{{ (row.change_perc * 100)|round|int }}%<span style="color: #999; font-size:10px">&nbsp({{row.change_count}})</span>
            </td>

            <td class="coverage">
              <div class="coverage_signal">
                <div class="coverage_bar" style="height:  4px; background: {% if row.coverage >  0 %}gray{% else %}lightgray{% endif %}"></div>
                <div class="coverage_bar" style="height:  8px; background: {% if row.coverage > 20 %}gray{% else %}lightgray{% endif %}"></div>
                <div class="coverage_bar" style="height: 12px; background: {% if row.coverage > 40 %}gray{% else %}lightgray{% endif %}"></div>
                <div class="coverage_bar" style="height: 16px; background: {% if row.coverage > 60 %}gray{% else %}lightgray{% endif %}"></div>
