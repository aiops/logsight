{
  "metadata": {
    "start": "field_parsing",
    "app_settings": {
      "application_id": "app_id",
      "private_key": "sample_key",
      "user_name": "sample_user",
      "application_name": "sample_app",
      "status": "create"
    }
  },
  "handlers": {
    "field_parsing": {
      "classname": "FieldParsingModule",
      "args": {
        "timeout_period": 30,
        "buffer_size": 10,
        "provider_threshold": 0.9
      },
      "next_handler": "log_parsing"
    },
    "log_parsing": {
      "classname": "LogParserModule",
      "args": {
        "timeout_period": 30,
          "buffer_size": 4096,
          "retrain_after": 200000
      },
      "next_handler": "anomaly_detection"
    },
    "anomaly_detection": {
      "classname": "AnomalyDetectionModule",
      "args": {
        "timeout_period": 20,
        "buffer_size": 16384
      },
      "next_handler": "log_aggregation"
    },
    "ad_sink": {
      "classname": "DataStoreModule",
      "args": {
        "sink": {
          "connection": "elasticsearch",
          "classname": "ElasticsearchSink",
          "params": {
            "index": "log_ad"
          }
        }
      },
      "next_handler": ""
    },
    "log_aggregation": {
      "classname": "LogAggregationModule",
      "args": {
        "timeout_period": 30,
        "buffer_size": 1000
      },
      "next_handler": "agg_sink"
    },
    "agg_sink": {
      "classname": "DataStoreModule",
      "args": {
        "sink": {
          "connection": "elasticsearch",
          "classname": "ElasticsearchSink",
          "params": {
            "index": "log_agg"
          }
        }
      },
      "next_handler": ""
    }
  }
}