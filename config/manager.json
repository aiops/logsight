{
  "connectors": {
    "source": {
      "connection": "kafka",
      "classname": "KafkaSource",
      "params": {
        "topic": "manager_settings"
      }
    },
    "producer": {
      "connection": "kafka",
      "classname": "KafkaSink",
      "params": {
        "topic": "manager_settings_ack"
      }
    }
  },
  "services": {
    "elasticsearch_admin": {
      "classname": "ElasticSearchAdmin",
      "connection": "elasticsearch"
    },
    "kafka_admin": {
      "classname": "KafkaAdmin",
      "connection": "kafka"
    },
    "database": {
      "classname": "PostgresDBConnection",
      "connection": "postgres"
    }
  },
  "topic_list": [
    "manager_settings",
    "manager_settings_ack",
    "internal",
    "log_quality_settings",
    "log_quality_ack",
    "container_settings_ack",
    "application_stats",
    "logsight.logstash.logsight_json",
    "logsight.logstash.logstash_json",
    "logsight.logstash.syslog",
    "logsight.log_receiver"
  ]
}